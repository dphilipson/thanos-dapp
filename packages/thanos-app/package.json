{
  "name": "thanos-app",
  "version": "1.0.0",
  "private": true,
  "description": "thanos-app",
  "author": "David Philipson",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean && rm -rf src/sc-artifacts/*",
    "develop": "gatsby develop",
    "format": "prettier --write '**/*.{js,json,md,ts,tsx}'",
    "pre-commit": "lint-staged",
    "serve": "gatsby serve",
    "start": "gatsby develop"
  },
  "lint-staged": {
